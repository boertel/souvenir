	"scripts": {
		"dev": "wrangler pages dev --compatibility-date=2023-11-21 --proxy 5174 -- pnpm exec vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
